<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="Main" type="Application" factoryName="Application">
    <option name="MAIN_CLASS_NAME" value="${package}.Main" />
    <module name="${rootArtifactId}-app" />
    <option name="VM_PARAMETERS" value="--add-modules=ALL-MODULE-PATH -cp ." />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="${package}.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>